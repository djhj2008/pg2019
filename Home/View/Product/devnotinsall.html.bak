<style>
	b {color: #23527C;}
</style>
<body>
	<!-- <h1>设备展示页面</h1>
 -->
 	<div class="container" >
 	<div class="navbar navbar-default navbar-fixed-left" >
		<ul class="nav navbar-nav" >
			<li class="active"><a href="<%$smarty.const.__APP__%>/devselect/select"><b >首页</b></a></li>
			<li><a href="<%$smarty.const.__APP__%>/acadd/checkrssi"><b>基站RSSI</b></a></li>
			</ul>
		</div>
		</div>
		
		<div class="container" >
     <table id="datatable-buttons" class="table table-striped table-bordered" cellspacing="0" width="100%">
     <thead>
			<tr>
				<th>SN</th>
        <th>psn</th>
        <th>devid</th>
        <th>状态</th>
        <th>ACCTION</th>
			</tr>
		</thead>
		  <tbody>
			<%foreach $dev_not_install as $value%>
			<tr>
				<td><%$value['sn']%></td>
        <td><%$value['psn']%></td>
        <td><%$value['devid']%></td>
        <td>未安装</td>
				<td>
				<a href="<%$smarty.const.__APP__%>/product/querytemp?devid=<%$value['devid']%>&psn=<%$value['psn']%>">查看温度</a>
				</td>
			</tr>
			<%/foreach%>
			<%foreach $devlist_err as $value%>
			<tr>
				<td><%$value['sn']%></td>
        <td><%$value['psn']%></td>
        <td><%$value['devid']%></td>
        <td>不合法</td>
				<td>
				<a href="<%$smarty.const.__APP__%>/product/querytemp?devid=<%$value['devid']%>&psn=<%$value['psn']%>">查看温度</a>
				</td>
			</tr>
			<%/foreach%>
		</tbody>
		</table>
		</div>
	<!--div class="container" style="margin-top: 50px"-->
</body>
